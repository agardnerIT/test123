{
    "name": "demo SRG",
    "description": "description for demo SRG",
    "tags": [
      "stage:production",
      "application:easytrade",
      "deployment:k8s-workload"
    ],
    "objectives": [
      {
        "name": "System load",
        "description": "...",
        "objectiveType": "DQL",
        "dqlQuery": "timeseries val = avg(dt.host.cpu.load), filter: in(dt.entity.host, {\"HOST-387F004C894DDC71\"}) | fields avg = arrayAvg(val)",
